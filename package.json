    "node-forge": "^0.10.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.5.0",
    "socket.io": "^2.4.1",
