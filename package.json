{
  "name": "gatewebapi",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "auto": "./node_modules/nodemon/bin/nodemon.js",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.spec.ts",
    "test-dev": "cross-env NODE_ENV=windows nodemon -L --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.spec.ts\"",
    "config-test-dev": "cp -R lib/views dist && cp -R public dist/public && DEBUG=bajs NODE_ENV=test nodemon --watch . --ext ts --exec \"env-cmd -f ./config/.env  mocha -r ts-node/register test/**/*.spec.ts\"",
    "comment_build": "tsc && cp ../config/cert.pem config/serverCert.pem",
    "build": "tsc",
    "dev": "ts-node ./lib/server.ts",
    "start": "nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "postinstall": "node-config-ts",
    "windows": "cross-env NODE_ENV=windows ts-node ./lib/server.ts",
    "watch": "tsc -w -p ."
  },
  "author": "",
  "license": "ISC",
